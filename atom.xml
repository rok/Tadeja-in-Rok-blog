---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Tadeja, Rok, Blog</title>
 <link href="http://trb.bajta.org" rel="self"/>
 <link href="http://trb.bajta.org/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://trb.bajta.org/</id>
 <author>
   <name>Tadeja, Rok</name>
   <email>rok@mihevc.org</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://trb.bajta.org{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://trb.bajta.org{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>